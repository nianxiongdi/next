{
    "title" : "fuson gitbook",
    "root":"./site",
    "links": {
        "gitbook": false,
        "sharing": {
          "google": false,
          "facebook": false,
          "twitter": false,
          "all": false
        }
    },
    "plugins": [ 
        "github",
        "versions-select",
        "-fontsettings"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/alibaba-fusion/next"
        },
        "fontsettings": {
            "theme": "white",
            "family": "sans",
            "size":  2
        },
        "versions": {
            "options": [
                {
                    "value": "http://localhost:4000/zh-ch/",
                    "text": "Chinese"
                },
                {
                    "value": "http://localhost:4000/en-us/",
                    "text": "English"
                }
            ]
        }
    } 
}